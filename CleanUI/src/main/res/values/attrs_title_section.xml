<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleSection">
        <attr name="title_section_title" format="string"/>
        <attr name="title_section_title_color" format="color"/>
        <attr name="title_section_title_size" format="float"/>
        <attr name="title_section_title_style">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
    </declare-styleable>
</resources>